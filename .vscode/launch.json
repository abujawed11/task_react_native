{
  "version": "0.2.0",
  "configurations": [

    {
      "name": "Debug Expo App",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:8080", // use your Expo devtools port
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    }
  ]
}
